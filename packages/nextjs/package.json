{
  "name": "@supaku/agentfactory-nextjs",
  "version": "0.7.31",
  "type": "module",
  "description": "Next.js API route handlers for AgentFactory â€” webhook processor, worker/session management, public stats",
  "author": "Supaku (https://supaku.com)",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supaku/agentfactory",
    "directory": "packages/nextjs"
  },
  "homepage": "https://github.com/supaku/agentfactory/tree/main/packages/nextjs",
  "bugs": {
    "url": "https://github.com/supaku/agentfactory/issues"
