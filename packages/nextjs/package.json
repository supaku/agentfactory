  "devDependencies": {
    "@types/node": "^22.5.4",
    "next": "^15.3.3",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build"
  }
